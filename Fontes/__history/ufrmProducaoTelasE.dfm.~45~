  object qry_aco: TFDQuery
    SQL.Strings = (
      'select'
      '  p.id, p.nome_fantasia, p.unidade, p.estoque_disponivel'
      'from'
      '  produtos p, departamentos d'
      'where'
      '  p.departamento_id = d.id and d.id = 2'
      'order by'
      '  p.nome_fantasia')
    Left = 432
    Top = 136
