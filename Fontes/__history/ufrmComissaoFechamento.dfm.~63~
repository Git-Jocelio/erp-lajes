    Active = True
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select '
      '  P.ID, V.PESSOA_ID, P.NOME'
      ' from '
      '  PESSOAS P, VENDEDORES V '
      ' where '
      '  P.ID = V.PESSOA_ID '
      ' order by P.NOME')
