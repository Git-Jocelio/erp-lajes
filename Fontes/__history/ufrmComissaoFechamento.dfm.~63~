  object qry_rel_debitos: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select'
      '  r.data_contabil,'
      '  r.descricao,'
      '  f.valor_debito'
      'from'
      '  fechamento_debitos f,'
      '  contas_receber r'
      'where'
      '  f.contas_receber_id = r.id  and'
      '  f.fechamento_comissao_id = :fechamento_comissao_id')
    Left = 139
    Top = 375
    ParamData = <
      item
        Name = 'FECHAMENTO_COMISSAO_ID'
        DataType = ftInteger
        ParamType = ptInput
      end>
  end
