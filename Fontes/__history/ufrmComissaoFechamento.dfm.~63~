  object qry_rel_comissoes: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select'
      ' c.id,'
      ' c.cadastrado_em,'
      ' comi.pedido_id,'
      ' pes.nome,'
      ' pes.bairro,'
      ' comi.total_venda,'
      ' comi.total_custo,'
      ' comi.comissao_liquida,'
      ' comi.historico'
      ''
      'from'
      ''
      ' fechamento_comissoes f,'
      ' fechamento_comissao c,'
      ' comissao comi,'
      ' pedidos ped,'
      ' pessoas pes'
      'where'
      ''
      ' c.id = f.fechamento_comissao_id and'
      ' f.comissao_id = comi.id and'
      ' comi.pedido_id = ped.id and'
      ' ped.cliente_id =  pes.id and'
      ' f.fechamento_comissao_id =:fechamento_comissao_id')
    Left = 106
    Top = 374
    ParamData = <
      item
        Name = 'FECHAMENTO_COMISSAO_ID'
        DataType = ftInteger
        ParamType = ptInput
      end>
  end
