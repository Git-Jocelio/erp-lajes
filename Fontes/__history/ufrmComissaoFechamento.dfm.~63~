  inherited qry: TFDQuery
    Filtered = True
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select '
      '  f.id, '
      '  f.cadastrado_em, '
      '  f.pessoa_id, '
      '  p.nome, '
      '  f.mes, '
      '  f.ano,'
      '  f.total_venda, '
      '  f.saldo_liquido, '
      '  f.obs'
      'from '
      '  fechamento_comissao f,'
      '  pessoas p '
      'where '
      '  f.pessoa_id = p.id ')
    Left = 64
    Top = 224
  end
