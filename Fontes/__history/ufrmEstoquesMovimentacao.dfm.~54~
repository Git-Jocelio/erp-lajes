  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      ' select'
      '   E.ESTOQUES_ID,'
      '   TABELA_ORIGEM,'
      '   TABELA_ORIGEM_ID, '
      '   E.PRODUTO_ID, '
      '   P.NOME_FANTASIA, '
      '   P.UNIDADE, '
      '   E.DATA, '
      '   E.HISTORICO, '
      '   E.ENTRADA, '
      '   E.SAIDA, '
      '   E.ESTOQUE_FISICO'
      ' from'
      '   ESTOQUES_MOVIMENTACAO E, PRODUTOS P'
      ' where'
      '  E.PRODUTO_ID = P.ID and               '
      '  P.ID = :PRODUTO_ID                    '
      'order by       '
      ' E.ESTOQUES_ID '
      '')
    Left = 632
    Top = 40
    ParamData = <
      item
        Name = 'PRODUTO_ID'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
