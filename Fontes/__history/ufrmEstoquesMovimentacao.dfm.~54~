  object qry_produtos: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select '
      '  P.ID,'
      '  P.NOME_FANTASIA'
      'from'
      ' PRODUTOS P'
      'where '
      '  REVENDA = '#39'S'#39' or VIGA = '#39'S'#39
      'order by '
      '  NOME_FANTASIA')
    Left = 568
    Top = 160
  end
