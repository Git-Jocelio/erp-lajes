  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select'
      
        '    i.id, i.pedido_id, pes.id,  pes.nome, ped.data_entrega, i.si' +
        'tuacao,'
      
        '    ped.vendedor_id, p.nome_fantasia, p.unidade, i.qtde, (i.qtde' +
        ' * i.venda) as total,  ped.pago'
      'from'
      '    pedidos_itens i, produtos p, PEDIDOS PED, PESSOAS PES'
      'where'
      '  PED.id = I.pedido_id and'
      '  I.produto_id = P.id AND'
      '  ped.cliente_id = pes.id and'
      '  (P.concreto = '#39'S'#39' or p.bomba = '#39'S'#39') '
      ''
      '')
    Left = 264
    Top = 312
