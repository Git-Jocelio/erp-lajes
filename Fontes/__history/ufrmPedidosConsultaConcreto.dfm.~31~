  object qry_clientes: TFDQuery
    SQL.Strings = (
      'select'
      '  P.ID,'
      '  P.NOME,'
      '  P.ENDERECO, '
      'P.NUMERO,'
      'P.BAIRRO,'
      'P.CIDADE'
      'from'
      '  PESSOAS P, CLIENTES C'
      'where'
      '  P.ID = C.pessoa_id'
      'order by'
      '  P.nome')
    Left = 192
    Top = 313
  end
  object qry_vendedores: TFDQuery
    SQL.Strings = (
      'select'
      '  P.ID,'
      '  P.NOME'
      'from'
      '  PESSOAS P, VENDEDORES V'
      'where'
      '  P.ID = V.PESSOA_ID'
      'order by'
      '  P.NOME')
    Left = 224
    Top = 313
  end
