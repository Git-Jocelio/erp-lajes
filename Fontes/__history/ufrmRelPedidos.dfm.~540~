    DataSet = qryComissoesFerrari
    BCDToCurrency = False
    DataSetOptions = []
    Left = 46
    Top = 490
  end
  object qryComissoesFerrari: TFDQuery
    SQL.Strings = (
      '/* comiss'#227'o antiga'
      'select '
      '  C.QUANTIDADE,'
      '  P.UNIDADE,'
      '  C.PRODUTO_ID AS COD_PROD,'
      '  P.DESCRICAO,'
      '  C.PRECO_VENDEDOR AS UNITARIO,'
      '  C.QUANTIDADE*C.PRECO_VENDEDOR AS TOTAL'
      ' '
      ' '
      'from '
      '  COMISSOES C,'
      '  PRODUTOS P'
      ''
      'where'
      '  C.PRODUTO_ID = P.ID and'
      '  C.PEDIDO_ID = :PEDIDO_ID'
      '*/'
      ''
      ''
      '/* comiss'#227'o segunda versao*/'
      '/*'
      'select '
      '  C.QUANTIDADE,'
      '  P.UNIDADE,'
      '  C.PRODUTO_ID AS COD_PROD,'
      '  P.DESCRICAO,'
      '  C.CUSTO_VEND AS UNITARIO,'
      '  C.QUANTIDADE * C.CUSTO_VEND AS TOTAL'
      ' '
      ' '
      'from '
      '  COMISSAO_ITEM C,'
      '  PRODUTOS P'
      ''
      'where'
      '  C.PRODUTO_ID = P.ID and'
      '  C.PEDIDO_ID = :PEDIDO_ID'
      '*/'
      '/* terceira vers'#227'o */'
      'select '
      '  sum( QUANTIDADE ) as QUANTIDADE,'
      '  P.UNIDADE,'
      '  C.PRODUTO_ID as COD_PROD,'
      '  P.DESCRICAO,'
      '  sum(quantidade*custo_vend) / sum(quantidade) AS UNITARIO ,'
      '  sum(quantidade*custo_vend) as TOTAL'
      'from'
      '  COMISSAO_ITEM C, PRODUTOS P'
      ''
      'where'
      '  C.PRODUTO_ID = P.ID and'
      '  C.PEDIDO_ID = :PEDIDO_ID'
      'group by '
      '  C.produto_id, P.DESCRICAO, P.unidade')
    Left = 46
    Top = 461
    ParamData = <
      item
        Name = 'PEDIDO_ID'
        DataType = ftString
        ParamType = ptInput
        Value = '40'
      end>
  end
  object frxDBComissoes_despesas_ferrari: TfrxDBDataset
    UserName = 'COMISSOES_DESPESAS_FERRARI'
