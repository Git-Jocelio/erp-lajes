  object qryPedidoItens: TFDQuery
    SQL.Strings = (
      'select '
      '  I.*,'
      '  I.QTDE*I.VENDA AS SUBTOTAL, '
      '  P.DESCRICAO,'
      '  P.UNIDADE '
      'from '
      '  PEDIDOS_ITENS I,'
      '  PRODUTOS P '
      'where '
      '  I.PRODUTO_ID = P.ID and '
      '  I.PEDIDO_ID = 212')
    Left = 360
    Top = 176
  end
