  object frxReportOrdemEntregaSimples: TfrxReport
    Version = '2022.2'
    DotMatrixReport = False
    IniFile = '\Software\Fast Reports'
    PreviewOptions.Buttons = [pbPrint, pbLoad, pbSave, pbExport, pbZoom, pbFind, pbOutline, pbPageSetup, pbTools, pbEdit, pbNavigator, pbExportQuick, pbCopy, pbSelection]
    PreviewOptions.Zoom = 1.000000000000000000
    PrintOptions.Printer = 'Padr'#227'o'
    PrintOptions.PrintOnSheet = 0
    ReportOptions.CreateDate = 45687.879625763890000000
    ReportOptions.LastChange = 45687.879625763890000000
    ScriptLanguage = 'PascalScript'
    ScriptText.Strings = (
      'procedure MasterData1OnBeforePrint(Sender: TfrxComponent);'
      'var'
      '  texto : string;                                       '
      'begin'
      ''
      '  texto := Copy( <cdsItensPedido."DESCRICAO">, 1, 4 );'
      ''
      '  if  texto = '#39'LAJE'#39' then'
      '  begin '
      '    cdsItensPedidoDESCRICAO.Font.style:= 1;'
      '    cdsItensPedidoDESCRICAO.Font.size:= 10;'
      '  end'
      '  else'
      '  begin              '
      '    cdsItensPedidoDESCRICAO.Font.style:= 0;'
      '    cdsItensPedidoDESCRICAO.Font.size:= 9;'
      '  end;'
      ''
      'end;'
      ''
      'procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);'
      'begin'
      '   if <Cliente."NOME"> = '#39'CONSUMIDOR'#39' then'
      '   begin'
      ''
      '     {esconde os dados do cliente quando for CONSUMIDOR}'
      
        '     // endere'#231'o e complemento                                  ' +
        '                   '
      '     memo46.visible            := false;'
      '     memo47.visible            := false;'
      '     memo48.visible            := false;'
      '     memo49.visible            := false;'
      '  '
      '     {posiciona os dados da entrega mais ao topo}'
      
        '     //dados da entrega                                         ' +
        ' '
      
        '     Memo45.top  := 83; // nome entrega                         ' +
        '                                                                ' +
        '                                                     '
      '     Memo43.top  := 98;'
      
        '     Memo44.top  := 78; // dados local entrega                  ' +
        '                                '
      '  '
      '     {tra'#231'o}           '
      '     memo50.top := 134;'
      
        '     ReportTitle1.Height := 135;                                ' +
        '                '
      '   end'
      '   else'
      '   if <Cliente."NOME"> <> '#39'CONSUMIDOR'#39' then'
      '   begin'
      
        '     {se n'#227'o for consumidor e o local de entrega for vazio, esco' +
        'nde o local de entrega}                             '
      '     if <local_entrega."ENDERECO"> = '#39#39' then'
      '     begin'
      
        '       memo45.visible := false;                                 ' +
        '                 '
      
        '       Memo43.visible := false;                                 ' +
        '                                                     '
      '       Memo44.visible := false;'
      '       {tra'#231'o}           '
      
        '       memo50.top          := 124;                              ' +
        '                                                        '
      '       ReportTitle1.Height := 125;'
      '     end else           '
      '     if <local_entrega."ENDERECO"> <> '#39#39' then'
      '     begin'
      
        '       // local de entrega                                      ' +
        '                             '
      
        '       memo45.visible := true;                                  ' +
        '                '
      
        '       Memo43.visible := true;                                  ' +
        '                                                    '
      '       Memo44.visible := true;'
      
        '       // posiciona endere'#231'o de entrega                         ' +
        '                                                   '
      
        '       Memo45.top  := 83;                                       ' +
        '                                               '
      '       Memo43.top  := 98;'
      '       Memo44.top  := 78;//113;'
      '  '
      '       //esconde endereco pessoa'
      '       memo46.visible := false;'
      '       memo47.visible := false;'
      '       memo48.visible := false;'
      '       memo49.visible := false;'
      '  '
      '       {tra'#231'o}           '
      
        '       memo50.top    := 134;                                    ' +
        '                                                  '
      '       ReportTitle1.Height := 135;'
      '     end;'
      '  '
      '   end;             '
      '  '
      'end;'
      ''
      'begin'
      ''
      '  '
      'end.')
    Left = 245
