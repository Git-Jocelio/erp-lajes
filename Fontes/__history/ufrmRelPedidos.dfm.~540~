  object qryPedItens: TFDQuery
    SQL.Strings = (
      'select                   '
      ' I.ID ,                  '
      ' I.ITEM ,                '
      ' I.NIVEL ,               '
      ' PED.NOSSO_NUMERO ,      '
      ' I.PRODUTO_ID AS CODIGO, '
      ' P.NOME_FANTASIA,        '
      ' P.UNIDADE AS UN,        '
      ' I.QTDE,                 '
      ' I.SITUACAO              '
      'from                     '
      ' PEDIDOS PED,            '
      ' PEDIDOS_ITENS I,        '
      ' PRODUTOS P              '
      'where                    '
      ' P.ID = I.PRODUTO_ID and '
      ' P.ID = I.PRODUTO_ID and I.PEDIDO_ID = PED.ID and  '
      ' P.REFORCO_DE_VIGA = '#39'N'#39' and  '
