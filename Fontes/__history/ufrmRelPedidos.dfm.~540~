  inherited qry: TFDQuery
    SQL.Strings = (
      ''
      ' select'
      ' P.ID,'
      ' P.NOSSO_NUMERO,'
      ' P.DATA_EMISSAO,'
      ' P.DATA_ENTREGA,'
      ' P.ORCAMENTO,'
      ' P.SITUACAO,'
      ' P.FORMA_PAGTO_ID,'
      ' F.DESCRICAO,'
      ' P.CLIENTE_ID,'
      ' P.VENDEDOR_ID,'
      ' P.VALOR_PRODUTOS,'
      ' P.FRETE_CLIENTE,'
      ' P.FRETE_EMITENTE,'
      ' P.DESCRICAO_ACRESCIMO,'
      ' P.ACRESCIMO,'
      ' P.DESCONTO,'
      ' P.VALOR_TOTAL_PEDIDO,'
      ' P.TRANSPORTADORA_ID,'
      ' P.VALOR_PAGO,'
      ' P.PAGO,'
      ' P.OBS_ADM,'
      ' P.OBS_ENTREGA,'
      ' V.COMISSAO_VENDEDOR,'
      ' P.VALOR_TOTAL_PEDIDO * V.comissao_adm / 100 AS COMISSAO_ADM'
      'from '
      ' PEDIDOS P, VENDEDORES V, FORMAS_PAGTO F'
      'where '
      '  P.FORMA_PAGTO_ID = F.ID and'
      '  P.vendedor_id = V.pessoa_id and'
      '  P.ID =:ID'
      '')
    Left = 198
    Top = 182
    ParamData = <
      item
        Name = 'ID'
        DataType = ftInteger
        ParamType = ptInput
        Value = 4018
      end>
  end
