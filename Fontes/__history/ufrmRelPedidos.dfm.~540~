  object qryTotalLaje: TFDQuery
    SQL.Strings = (
      'select'
      '  /* sum (V.COMPRIMENTO * i.QTDE * 0.42 / 1000) AS METROS  */'
      '  sum (V.COMPRIMENTO * i.QTDE) AS METROS '
      
        '  /* sum (V.COMPRIMENTO * I.QTDE * V.FORMA_MEDIDA) / 1000000 AS ' +
        'METROS */ '
      'from '
      '  PEDIDOS_ITENS_LAJE I, '
      '  PRODUTOS P,'
      '  PRODUTOS_VIGAS V '
      'where '
      '  P.ID = I.PRODUTO_ID and '
      '  P.ID = V.PRODUTO_ID and'
      '  P.VIGA = '#39'S'#39' and '
      '  I.PEDIDO_ID =:PEDIDO_ID and'
      '  I.ITEM =:ITEM and'
      '  V.TRELICA_ALTURA = :ALTURA /* and'
      '  V.FORMA_MEDIDA = :FORMA */')
    Left = 372
    Top = 274
    ParamData = <
      item
        Name = 'PEDIDO_ID'
        DataType = ftInteger
        ParamType = ptInput
        Value = 25
      end
      item
        Name = 'ITEM'
        DataType = ftInteger
        ParamType = ptInput
        Value = 3
      end
      item
        Name = 'ALTURA'
        DataType = ftInteger
        ParamType = ptInput
        Value = 120
      end>
  end
  object qryComissoes: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      '/* comiss'#227'o antiga'
      'select '
      '  C.QUANTIDADE,'
      '  P.UNIDADE,'
      '  C.PRODUTO_ID AS COD_PROD,'
      '  P.DESCRICAO,'
      '  C.PRECO_VENDEDOR AS UNITARIO,'
      '  C.QUANTIDADE*C.PRECO_VENDEDOR AS TOTAL'
      ' '
      ' '
      'from '
      '  COMISSOES C,'
      '  PRODUTOS P'
      ''
      'where'
      '  C.PRODUTO_ID = P.ID and'
      '  C.PEDIDO_ID = :PEDIDO_ID'
      '*/'
      ''
      ''
      '/* comiss'#227'o segunda versao*/'
      '/*'
      'select '
      '  C.QUANTIDADE,'
      '  P.UNIDADE,'
      '  C.PRODUTO_ID AS COD_PROD,'
      '  P.DESCRICAO,'
      '  C.CUSTO_VEND AS UNITARIO,'
      '  C.QUANTIDADE * C.CUSTO_VEND AS TOTAL'
      ' '
      ' '
      'from '
      '  COMISSAO_ITEM C,'
      '  PRODUTOS P'
      ''
      'where'
      '  C.PRODUTO_ID = P.ID and'
      '  C.PEDIDO_ID = :PEDIDO_ID'
      '*/'
      '/* terceira vers'#227'o */'
      'select '
      '  sum( QUANTIDADE ) as QUANTIDADE,'
      '  P.UNIDADE,'
      '  C.PRODUTO_ID as COD_PROD,'
      '  P.DESCRICAO,'
      '  sum(quantidade*custo_vend) / sum(quantidade) AS UNITARIO ,'
      '  sum(quantidade*custo_vend) as TOTAL'
      'from'
      '  COMISSAO_ITEM C, PRODUTOS P'
      ''
      'where'
      '  C.PRODUTO_ID = P.ID and'
      '  C.PEDIDO_ID = :PEDIDO_ID'
      'group by '
      '  C.produto_id, P.DESCRICAO, P.unidade')
    Left = 49
    Top = 365
    ParamData = <
      item
        Name = 'PEDIDO_ID'
        DataType = ftString
        ParamType = ptInput
        Value = '40'
      end>
  end
