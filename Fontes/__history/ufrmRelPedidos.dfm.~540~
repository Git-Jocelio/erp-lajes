  object qryConcreto: TFDQuery
    SQL.Strings = (
      
        'select i.id, i.pedido_id,  i.item,  i.nivel,  i.local, i.produto' +
        '_id, p.nome_fantasia, I.QTDE, p.unidade,'
      'i.situacao'
      'from pedidos_itens i, produtos p, PEDIDOS PED'
      'where'
      '  PED.id = I.pedido_id and'
      '  I.produto_id = P.id AND'
      '  (P.concreto = '#39'S'#39' or p.bomba = '#39'S'#39') and'
      '  i.pedido_id = :PEDIDO_ID'
      '  ORDER by I.item,  I.local')
    Left = 412
    Top = 403
    ParamData = <
      item
        Name = 'PEDIDO_ID'
        DataType = ftInteger
        ParamType = ptInput
      end>
    object qryConcretoID: TIntegerField
      FieldName = 'ID'
      Origin = 'ID'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
    end
    object qryConcretoPEDIDO_ID: TIntegerField
      FieldName = 'PEDIDO_ID'
      Origin = 'PEDIDO_ID'
      Required = True
    end
    object qryConcretoITEM: TIntegerField
      FieldName = 'ITEM'
      Origin = 'ITEM'
    end
    object qryConcretoNIVEL: TStringField
      FieldName = 'NIVEL'
      Origin = 'NIVEL'
      Size = 15
    end
    object qryConcretoLOCAL: TStringField
      FieldName = 'LOCAL'
      Origin = '"LOCAL"'
      Size = 4
    end
    object qryConcretoPRODUTO_ID: TIntegerField
      FieldName = 'PRODUTO_ID'
      Origin = 'PRODUTO_ID'
      Required = True
    end
    object qryConcretoNOME_FANTASIA: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'NOME_FANTASIA'
      Origin = 'NOME_FANTASIA'
      ProviderFlags = []
      ReadOnly = True
      Size = 100
    end
    object qryConcretoUNIDADE: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'UNIDADE'
      Origin = 'UNIDADE'
      ProviderFlags = []
      ReadOnly = True
      Size = 2
    end
    object qryConcretoSITUACAO: TStringField
      FieldName = 'SITUACAO'
      Origin = 'SITUACAO'
      Required = True
      Size = 15
    end
    object qryConcretoQTDE: TCurrencyField
