  object qryCliente: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select'
      '  P.*,'
      '  C.PESSOA_ID AS CLIENTE_ID'
      'from '
      '  CLIENTES C,'
      '  PESSOAS P'
      'where '
      '  C.PESSOA_ID = P.ID and '
      '  C.PESSOA_ID = 6 ')
    Left = 232
    Top = 80
  end
  object qryVendedor: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select'
      '  V.PESSOA_ID AS VENDEDOR_ID,'
      '  P.NOME'
      ''
      'from'
      '  VENDEDORES V,'
      '  PESSOAS P '
      'where'
      '  V.PESSOA_ID = P.ID and '
      '  V.PESSOA_ID = 3 ')
    Left = 264
    Top = 80
  end
