  object frxReportComissoesFerrari: TfrxReport
    Version = '2022.2'
    DotMatrixReport = False
    IniFile = '\Software\Fast Reports'
    PreviewOptions.Buttons = [pbPrint, pbLoad, pbSave, pbExport, pbZoom, pbFind, pbOutline, pbPageSetup, pbTools, pbEdit, pbNavigator, pbExportQuick, pbCopy, pbSelection]
    PreviewOptions.Zoom = 1.000000000000000000
    PrintOptions.Printer = 'Default'
    PrintOptions.PrintOnSheet = 0
    ReportOptions.CreateDate = 44371.837864548600000000
    ReportOptions.LastChange = 45337.614443842590000000
    ScriptLanguage = 'PascalScript'
    ScriptText.Strings = (
      'procedure Picture1OnAfterData(Sender: TfrxComponent);'
      'begin'
      
        '  picture1.Picture.LoadFromFile ( <imagem_rel."REL_PEDIDO_IMG_TO' +
        'PO">  );'
      'end;'
      ''
      'procedure MasterData1OnBeforePrint(Sender: TfrxComponent);'
      'begin'
      
        '    memo36.text := FormatFloat('#39'0.00'#39',  (<comissoes_ferrari."tot' +
        'al"> / <comissoes_ferrari."unitario">) );'
      ''
      'end;'
      ''
      'procedure Child1OnBeforePrint(Sender: TfrxComponent);'
      'begin'
      '   if <Cliente."NOME"> = '#39'CONSUMIDOR'#39' then'
      '   begin'
      ''
      '     {esconde os dados do cliente quando for CONSUMIDOR}'
      '     memo4.visible := false;'
      '     pedidoCPF_CNPJ.Visible   := false;'
      '     clienteRG_IE.Visible     := false;'
