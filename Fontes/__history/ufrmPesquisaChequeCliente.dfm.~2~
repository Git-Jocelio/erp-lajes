  inherited qry: TFDQuery
    SQL.Strings = (
      'SELECT'
      '  p1.id as pessoa_id,'
      '  cc.id,'
      '  cc.cadastrado_em,'
      '  p1.NOME AS NOME_CORRENTISTA, '
      '  p2.NOME AS NOME_BANCO,'
      '  c.nr_agencia,'
      '  c.nr_conta,'
      '  cc.NR_CHEQUE,'
      '  cc.valor,'
      '  cc.pre_datado_para,'
      '  cc.historico,'
      '  cc.repassado'
      'FROM '
      '  CHEQUES_CLIENTES cc, '
      '  CORRENTISTA_CONTAS c, '
      '  PESSOAS p1, '
      '  BANCOS b, '
      '  PESSOAS p2'
      'WHERE '
      '  cc.CORRENTISTA_CONTA_ID = c.ID AND '
      '  c.PESSOA_ID = p1.ID AND '
      '/*  c.BANCO_ID = b.ID AND */ '
      '  c.BANCO_ID = b.pessoa_ID AND '
      '  b.PESSOA_ID = p2.ID and'
      '  cc.repassado =:REPASSADO')
    Left = 120
    Top = 168
    ParamData = <
      item
        Name = 'REPASSADO'
        ParamType = ptInput
      end>
  end
  inherited ds: TDataSource
    Left = 150
    Top = 168
  end
