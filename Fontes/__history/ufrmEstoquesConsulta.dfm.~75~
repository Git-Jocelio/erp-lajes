  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select'
      ' P.ID,'
      ' P.NOME_FANTASIA,'
      ' P.UNIDADE,'
      ' P.PRECO_CUSTO,'
      ' P.PERCA,'
      ' P.CUSTO_LIQUIDO,'
      ' P.PRECO_VENDEDOR,'
      ' P.PRECO_VENDA,'
      ' P.TEMPO_REPOSICAO,'
      ' P.QTDE_MIN,'
      ' P.QTDE_MAX,'
      ' P.PONTO_PEDIDO,'
      ' P.ESTOQUE_FISICO,'
      ' P.PEDIDO_ABERTO,'
      ' P.ESTOQUE_DISPONIVEL,'
      ' P.PEDIDO_AGUARDANDO,'
      ' P.ESTOQUE_LIQUIDO'
      'from'
      ' PRODUTOS P,'
      ' PRODUTOS_VIGAS V'
      'where'
      ' P.ID = V.PRODUTO_ID and'
      ' V.FORMA_MEDIDA =:FORMA_MEDIDA and'
      ' V.TRELICA_ALTURA =:TRELICA_ALTURA')
    Left = 96
    Top = 328
    ParamData = <
      item
        Name = 'FORMA_MEDIDA'
        DataType = ftInteger
        ParamType = ptInput
      end
      item
        Name = 'TRELICA_ALTURA'
        DataType = ftInteger
        ParamType = ptInput
      end>
