  object qry_movimento: TFDQuery
    SQL.Strings = (
      ' select                               '
      '   E.ESTOQUES_ID,                     '
      '   E.TABELA_ORIGEM,                   '
      '   E.TABELA_ORIGEM_ID,                '
      '   E.PRODUTO_ID,                      '
      '   P.NOME_FANTASIA,                   '
      '   P.UNIDADE,                         '
      '   E.DATA,                            '
      '   E.HISTORICO,                       '
      '   E.ENTRADA,                         '
      '   E.SAIDA,                           '
      '   E.ESTOQUE_FISICO                   '
      ' from                                 '
      '   ESTOQUES_MOVIMENTACAO E, PRODUTOS P'
      ' where                                '
      '  E.PRODUTO_ID = P.ID and             '
      '  P.ID = :PRODUTO_ID                  '
      ' order by                             '
      '  E.ESTOQUES_ID                       '
      '')
    Left = 141
    Top = 329
    ParamData = <
      item
        Name = 'PRODUTO_ID'
        ParamType = ptInput
      end>
  end
