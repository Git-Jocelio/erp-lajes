  object qry_composicao: TFDQuery
    SQL.Strings = (
      'select'
      '  mp.nome_fantasia as materia_prima,'
      '  c.quantidade,'
      '  mp.unidade'
      'from'
      '  PRODUTOS_VIGAS_COMPOSICAO c, produtos mp'
      ''
      'where'
      '  mp.id = c.mp_id and'
      '  c.produto_id = :produto_id')
