object dmPesquisa: TdmPesquisa
  OnCreate = DataModuleCreate
  Height = 438
  Width = 587
  object qryBuscaProduto: TFDQuery
    CachedUpdates = True
    SQL.Strings = (
      'select * from PRODUTOS where ID =:ID')
    Left = 48
    Top = 96
    ParamData = <
      item
        Name = 'ID'
        DataType = ftInteger
        ParamType = ptInput
      end>
  end
  object qryBuscaComposicaoProduto: TFDQuery
    CachedUpdates = True
    SQL.Strings = (
      
        'select * from PRODUTOS_VIGAS_COMPOSICAO where PRODUTO_ID=:PRODUT' +
        'O_ID')
    Left = 224
    Top = 80
    ParamData = <
      item
        Name = 'PRODUTO_ID'
        DataType = ftInteger
        ParamType = ptInput
      end>
  end
  object qryAux: TFDQuery
    Left = 48
    Top = 32
  end
end
