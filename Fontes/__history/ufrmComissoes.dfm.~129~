  object qry_comissao_itens_agrupados: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'SELECT'
      ' C.PRODUTO_ID, P.NOME_FANTASIA,'
      ' SUM(QUANTIDADE),  P.unidade'
      ''
      ' from COMISSAO_ITEM C, produtos P'
      ''
      ' where'
      '  C.produto_id = P.id AND'
      '  pedido_id = 421'
      'GROUP BY C.produto_id, P.NOME_FANTASIA, P.unidade')
    Left = 300
    Top = 342
    object qry_comissao_itens_agrupadosPRODUTO_ID: TIntegerField
      FieldName = 'PRODUTO_ID'
      Origin = 'PRODUTO_ID'
