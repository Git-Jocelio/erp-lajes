  inherited ds: TDataSource
    Left = 184
    Top = 312
  end
  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      '/*****'
      'select                      '
      '   C.id,                    '
      '   vend.nome as nm_vendedor,'
      '   C.data_contabil,         '
      '   C.pedido_id,             '
      '   P.NOSSO_NUMERO,          '
      '   cli.nome as nm_cliente,  '
      '   cli.bairro,              '
      '   C.vendedor_id,           '
      '   C.total_venda,           '
      '   C.total_custo,           '
      '   C.outras_despesas,       '
      '   C.COMISSAO_ADM_TAXA,     '
      '   C.comissao_adm,          '
      '   C.COMISSAO_BRUTA,        '
      '   C.comissao_liquida,      '
      '   C.status,                '
      '   C.historico              '
      ' from                       '
      '   comissao C,              '
      '   pedidos P,               '
      '   pessoas cli,             '
      '   pessoas vend             '
      ' where                      '
      '   c.pedido_id = P.ID and   '
      '   cli.id = p.cliente_id and'
      '   c.vendedor_id = vend.id  '
      '****/')
    Left = 184
    Top = 344
  end
