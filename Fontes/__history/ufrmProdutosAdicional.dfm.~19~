  inherited qry: TFDQuery
    Connection = dmConn.Conn
    FormatOptions.AssignedValues = [fvFmtDisplayNumeric]
    FormatOptions.FmtDisplayNumeric = '0.00'
    SQL.Strings = (
      'select'
      '  P.ATIVO,'
      '  P.ID,'
      '  P.NOME_FANTASIA,'
      '  A.PRECO_CUSTO_12MT,'
      '  P.PERCA,'
      '  P.CUSTO_LIQUIDO*12 as CUSTO_LIQUIDO,'
      '  P.PRECO_VENDA'
      'from '
      '  PRODUTOS P,'
      '  PRODUTOS_ADICIONAL A '
      'where'
      '  P.ID = A.PRODUTO_ID and '
      '  P.ID = -1')
    Left = 48
    Top = 216
  end
