  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select'
      '  R.ID,'
      '  R.DATA_EMI,'
      '  R.DATA_CONTABIL,'
      '  P.NOME,'
      '  R.DESCRICAO,'
      '  R.TOTAL,'
      '  R.VALOR_PAGO,'
      '  R.SALDO_ABERTO'
      'from'
      '  CONTAS_RECEBER R, PESSOAS P'
      'where'
      '  R.PESSOA_ID = P.ID  ')
    Left = 648
    Top = 40
    object qryID: TIntegerField
      FieldName = 'ID'
      Origin = 'ID'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
