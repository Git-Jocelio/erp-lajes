  object qryTotalLaje: TFDQuery
    SQL.Strings = (
      'select'
      ''
      '  sum ( V.COMPRIMENTO  *  i.QTDE  *  0.42  / 1000 ) AS METROS '
      ''
      'from '
      ''
      '  PEDIDOS_ITENS_LAJE I, '
      '  PRODUTOS P,'
      '  PRODUTOS_VIGAS V '
      ''
      'where '
      ''
      '  P.ID = I.PRODUTO_ID and '
      '  P.ID = V.PRODUTO_ID and'
      '  P.VIGA = '#39'S'#39'and '
      '  I.PEDIDO_ID =:PEDIDO_ID and'
      '  I.ITEM =:ITEM and'
      '  V.TRELICA_ALTURA = :ALTURA')
    Left = 922
    Top = 581
    ParamData = <
      item
        Name = 'PEDIDO_ID'
        DataType = ftString
        ParamType = ptInput
        Value = '25'
      end
      item
        Name = 'ITEM'
        DataType = ftSmallint
        ParamType = ptInput
        Value = 3
      end
      item
        Name = 'ALTURA'
        DataType = ftString
        ParamType = ptInput
        Value = '120'
      end>
