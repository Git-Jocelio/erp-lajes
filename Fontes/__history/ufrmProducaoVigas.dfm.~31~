  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select'
      '  v.trelica_altura,'
      '  cast ( v.comprimento as float ) /1000 as comprimento,'
      '  sum(i.qtde) as qtde'
      'from'
      '  pedidos_itens_laje i,'
      '  produtos_vigas v,'
      '  pedidos p'
      'where'
      '  i.pedido_id = p.id and'
      '  i.produto_id = v.produto_id and'
      '  p.nosso_numero in (:p1,:p2,:p3)'
      'group by'
      '  v.trelica_altura, v.comprimento')
    Left = 189
    Top = 29
    ParamData = <
      item
        Name = 'P1'
        DataType = ftString
        ParamType = ptInput
        Size = 20
      end
      item
        Name = 'P2'
        DataType = ftString
        ParamType = ptInput
        Size = 20
      end
      item
        Name = 'P3'
        DataType = ftString
        ParamType = ptInput
        Size = 20
      end>
    object qryTRELICA_ALTURA: TIntegerField
      FieldName = 'TRELICA_ALTURA'
      Origin = 'TRELICA_ALTURA'
    end
    object qryQTDE: TLargeintField
      FieldName = 'QTDE'
      Origin = 'QTDE'
    end
    object qryCOMPRIMENTO: TFloatField
      FieldName = 'COMPRIMENTO'
      Origin = 'COMPRIMENTO'
      DisplayFormat = '0.00'
      EditFormat = '0.00'
    end
  end
