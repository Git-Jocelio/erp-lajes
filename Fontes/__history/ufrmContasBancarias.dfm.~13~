  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select                  '
      '  c.id,                 '
      '  c.cadastrado_em,      '
      '  c.alterado_em,        '
      '  b.cod_banco,          '
      '  p.nome,               '
      '  c.ativo               '
      'from                    '
      '  contas_bancarias c,   '
      '  bancos b,             '
      '  pessoas p             '
      'where                   '
      '  c.banco_id = b.id and '
      '  p.id = b.pessoa_id    ')
