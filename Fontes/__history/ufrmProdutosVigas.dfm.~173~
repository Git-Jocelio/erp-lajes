  object qryFiltraMp: TFDQuery
    SQL.Strings = (
      'select'
      ' P.ID,'
      ' P.DESCRICAO,'
      ' V.COMPRIMENTO,'
      ' C.ID AS ID_COMP,'
      ' C.MP_ID,'
      ' C.QUANTIDADE'
      'FROM'
      ' PRODUTOS P,'
      ' PRODUTOS_VIGAS V,'
      ' PRODUTOS_VIGAS_COMPOSICAO C'
      'WHERE'
      ' P.ID = V.PRODUTO_ID and'
      ' P.ID = C.PRODUTO_ID and'
      ' V.PRODUTO_ID = C.PRODUTO_ID and'
      ' C.MP_ID = :COD_MP')
    Left = 200
    Top = 408
    ParamData = <
      item
        Name = 'COD_MP'
        DataType = ftInteger
        ParamType = ptInput
      end>
  end
  object qryAtualizaMp: TFDQuery
