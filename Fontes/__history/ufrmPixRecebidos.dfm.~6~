  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select'
      '  p.id,'
      '  p.data_pix,'
      '  p.contas_bancarias_itens_id,'
      '  c.banco_id,'
      '  p1.nome as nome_banco,'
      '  c.agencia,'
      '  c.nr_conta,'
      '  p.valor,'
      '  p.obs'
      'from'
      '  pix_recebidos p,'
      '  contas_bancarias_itens c,'
      '  pessoas p1,'
      '  bancos b'
      'where'
      '  c.id =  p.contas_bancarias_itens_id and'
      '  c.banco_id = b.id and'
      '  b.pessoa_id = p1.id')
    object qryID: TIntegerField
      FieldName = 'ID'
      Origin = 'ID'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
    end
