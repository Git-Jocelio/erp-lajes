  inherited qry: TFDQuery
    Left = 376
    Top = 128
  end
  inherited ds: TDataSource
    Left = 376
    Top = 89
  end
  object qry_contas: TFDQuery
    SQL.Strings = (
      'select'
      '   p1.id as pessoa_id,'
      '   cc.id,'
      '   p2.nome as nome_banco,'
      '   cc.nr_agencia,'
      '   cc.nr_conta'
      'from'
      '  correntista_contas cc,'
      '  pessoas p1,'
      '  bancos b,'
      '  pessoas p2'
      'where'
      '  p1.id = cc.pessoa_id and'
      '/*  cc.banco_id = b.id and */'
      '  cc.banco_id = b.pessoa_id and '
      '  p2.id = b.pessoa_id and'
      '  p1.cpf_cnpj like :cpf_cnpj')
    Left = 424
    Top = 129
    ParamData = <
      item
        Name = 'CPF_CNPJ'
        DataType = ftString
        ParamType = ptInput
        Size = 18
      end>
  end
  object ds_contas: TDataSource
    AutoEdit = False
    DataSet = qry_contas
    OnDataChange = ds_contasDataChange
    Left = 424
    Top = 89
  end
