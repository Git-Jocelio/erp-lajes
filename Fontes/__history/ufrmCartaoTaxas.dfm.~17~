  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select                                '
      '  t.id,                               '
      '  t.cadastrado_em,                    '
      '  t.alterado_em,                      '
      '  p.nome as descricao_operadora, '
      '  b.descricao as descricao_bandeira,  '
      '  t.ativo                             '
      'from                                  '
      '  cartao_taxas t,                     '
      '  cartao_operadoras o,                '
      '  cartao_bandeiras b,                  '
      '  pessoas p                        '
      'where                                 '
      '  p.id = o.pessoa_id and          '
      '  t.cartao_operadoras_id = o.id and   '
      '  t.cartao_bandeiras_id = b.id        ')
