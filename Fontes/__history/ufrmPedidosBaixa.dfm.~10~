  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select'
      '  PD.ID,'
      '  PD.DATA_EMISSAO,'
      '  PD.DATA_ENTREGA,'
      '  P.NOME,'
      '  P.ENDERECO,'
      '  P.NUMERO,'
      '  P.BAIRRO,'
      '  P.CIDADE'
      'from'
      '  PEDIDOS PD, PESSOAS P'
      'where'
      '  PD.CLIENTE_ID = P.ID and'
      '  PD.ORCAMENTO = '#39'N'#39' and'
      '  PD.SITUACAO = '#39'ABERTO'#39
      ''
      '')
    Left = 96
    Top = 192
  end
  object ds: TDataSource
    AutoEdit = False
    DataSet = qry
    OnDataChange = dsDataChange
    Left = 128
    Top = 193
  end
