      '  A.PRECO_CUSTO_12MT/12 AS CUSTO_METRO,'
      '  P.PERCA,'
      '  P.CUSTO_LIQUIDO,'
      '  P.PRECO_VENDEDOR,'
      '  P.PRECO_VENDA'
      'from '
      '  PRODUTOS P,'
      '  PRODUTOS_ADICIONAL A '
      'where'
      '  P.ID = A.PRODUTO_ID and '
      '  P.ID = -1'
      '')
