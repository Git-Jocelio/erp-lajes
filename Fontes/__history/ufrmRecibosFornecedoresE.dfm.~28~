  inherited qry: TFDQuery
    SQL.Strings = (
      'select '
      '  p.id, '
      '  p.nome '
      'from '
      '  pessoas p,'
      '  fornecedores f'
      'where '
      '  f.pessoa_id = p.id'
      'order by nome')
    Left = 408
    Top = 72
  end
