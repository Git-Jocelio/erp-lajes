  object qry_bancos: TFDQuery
    SQL.Strings = (
      'select '
      '  b.id,'
      '  b.pessoa_id,'
      '  b.cod_banco,'
      '  p.nome'
      'from'
      '  bancos b,'
      '  pessoas P'
      'where '
      '  b.pessoa_id = p.id'
      'order by'
      '  b.cod_banco')
    Left = 696
    Top = 229
  end
  object qry_contas_bancarias: TFDQuery
    SQL.Strings = (
      'select '
      '  id,'
      '  banco_id,'
      '  agencia,'
      '  nr_conta'
      'from'
      ' contas_bancarias_itens'
      'where'
      '  banco_id =:banco_id')
    Left = 723
    Top = 229
    ParamData = <
      item
        Name = 'BANCO_ID'
        ParamType = ptInput
      end>
  end
