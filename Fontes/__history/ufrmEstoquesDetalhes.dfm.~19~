  object qry_itens_pedidos: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      '/*select'
      '    it.pedido_id,'
      '    it.produto_id,'
      '    prod.nome_fantasia,'
      '    it.Qtde'
      'from'
      '    pedidos_itens it, produtos prod'
      'where'
      '    it.produto_id = prod.id and'
