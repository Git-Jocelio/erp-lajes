    Top = 120
  end
  object qry_detalhes_pedido: TFDQuery
    AfterScroll = qry_detalhes_pedidoAfterScroll
    Connection = dmConn.FDConnection
    SQL.Strings = (
      '/*LISTA TODOS OS PEDIDOS ABERTO OU AGUARDANDO'
      '  QUE CONTEM A VIGA PASSADA COMO PARAMETRO*/'
      'SELECT'
      '   IP.PEDIDO_ID ,'
      '   P.SITUACAO,'
      '   P.DATA_ENTREGA,'
      '  /* ip.produto_id, */'
      '   IP.QTDE,'
      '   PS.NOME'
      'FROM'
      '  PEDIDOS P, PEDIDOS_ITENS IP , PESSOAS PS'
      ''
      'WHERE'
      '  ( IP.PEDIDO_ID = P.ID ) and'
      '  ( P.cliente_id = PS.id ) AND'
      
        '  ( (p.situacao = '#39'ABERTO'#39') or (p.situacao = '#39'PARCIAL'#39') or (p.si' +
        'tuacao = '#39'AGUARDANDO'#39') ) and'
      '   ip.produto_id = :produto_id')
    Left = 168
    Top = 291
    ParamData = <
      item
        Name = 'PRODUTO_ID'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
    object qry_detalhes_pedidoPEDIDO_ID: TIntegerField
      FieldName = 'PEDIDO_ID'
      Origin = 'PEDIDO_ID'
      Required = True
    end
    object qry_detalhes_pedidoSITUACAO: TStringField
      FieldName = 'SITUACAO'
      Origin = 'SITUACAO'
      Size = 15
    end
    object qry_detalhes_pedidoDATA_ENTREGA: TDateField
      FieldName = 'DATA_ENTREGA'
      Origin = 'DATA_ENTREGA'
    end
    object qry_detalhes_pedidoQTDE: TCurrencyField
      AutoGenerateValue = arDefault
      FieldName = 'QTDE'
      Origin = 'QTDE'
      ProviderFlags = []
      ReadOnly = True
      currency = False
    end
    object qry_detalhes_pedidoNOME: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'NOME'
      Origin = 'NOME'
      ProviderFlags = []
      ReadOnly = True
      Size = 100
    end
  end
  object qry_detalhes_laje: TFDQuery
    AfterScroll = qry_detalhes_lajeAfterScroll
    Connection = dmConn.FDConnection
    SQL.Strings = (
      '/*LISTA TODOS OS PEDIDOS ABERTO OU AGUARDANDO'
      '  QUE CONTEM A VIGA PASSADA COMO PARAMETRO*/'
      'SELECT'
      '   IP.PEDIDO_ID,'
      '   P.SITUACAO,'
      '   P.DATA_ENTREGA,'
      '   ip.produto_id,'
      '   IP.QTDE,'
      '   PS.NOME'
      'FROM'
      '  PEDIDOS P, pedidos_itens_laje IP , PESSOAS PS'
      ''
      'WHERE'
      '  ( IP.PEDIDO_ID = P.ID ) and'
      '  ( P.cliente_id = PS.id ) AND'
      
        '  ( (p.situacao = '#39'ABERTO'#39') or (p.situacao = '#39'PARCIAL'#39') or (p.si' +
        'tuacao = '#39'AGUARDANDO'#39') ) and'
      '   ip.produto_id = :produto_id')
    Left = 168
    Top = 459
    ParamData = <
      item
        Name = 'PRODUTO_ID'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
    object qry_detalhes_lajePEDIDO_ID: TIntegerField
      FieldName = 'PEDIDO_ID'
      Origin = 'PEDIDO_ID'
      Required = True
    end
