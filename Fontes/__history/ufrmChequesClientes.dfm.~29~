  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'SELECT'
      '  p1.id as pessoa_id,'
      '  cc.id,'
      '  cc.cadastrado_em,'
      '  p1.NOME AS NOME_CORRENTISTA, '
      '  p2.NOME AS NOME_BANCO,'
      '  c.nr_agencia,'
      '  c.nr_conta,'
      '  cc.NR_CHEQUE,'
      '  cc.valor,'
      '  cc.pre_datado_para,'
      '  cc.historico,'
      '  cc.repassado'
      'FROM '
      '  CHEQUES_CLIENTES cc, '
      '  CORRENTISTA_CONTAS c, '
      '  PESSOAS p1, '
      '  BANCOS b, '
      '  PESSOAS p2'
      'WHERE '
      '  cc.CORRENTISTA_CONTA_ID = c.ID AND '
      '  c.PESSOA_ID = p1.ID AND '
      '/*  c.BANCO_ID = b.ID AND */ '
      '  c.BANCO_ID = b.pessoa_ID AND '
      '  b.PESSOA_ID = p2.ID'
      '  ')
    object qryID: TIntegerField
      FieldName = 'ID'
      Origin = 'ID'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
    end
    object qryPESSOA_ID: TIntegerField
      FieldName = 'PESSOA_ID'
      Origin = 'PESSOA_ID'
      Required = True
    end
    object qryCADASTRADO_EM: TDateField
      FieldName = 'CADASTRADO_EM'
      Origin = 'CADASTRADO_EM'
    end
