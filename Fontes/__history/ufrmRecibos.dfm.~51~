  inherited qry: TFDQuery
    Connection = dmConn.FDConnection
    SQL.Strings = (
      'select'
      '  r.recibo_id,'
      '  p.nome,'
      '  P.CPF_CNPJ,'
      '  r.cadastrado_em,'
      '  r.valor,'
      '  r.observacao,'
      '  r.valor_extenso'
      'from'
      '  recibos r,'
      '  pessoas p'
      'where'
      '  r.pessoa_id = p.id'
      '')
