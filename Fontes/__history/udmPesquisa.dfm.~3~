object dmPesquisa: TdmPesquisa
  OldCreateOrder = False
  OnCreate = DataModuleCreate
  Height = 438
  Width = 587
  object qryBuscaProduto: TFDQuery
    SQL.Strings = (
      'select * from PRODUTOS where ID =:ID')
    Left = 56
    Top = 24
    ParamData = <
      item
        Name = 'ID'
        DataType = ftInteger
        ParamType = ptInput
      end>
  end
  object qryBuscaComposicaoProduto: TFDQuery
    SQL.Strings = (
      
        'select * from PRODUTOS_VIGAS_COMPOSICAO where PRODUTO_ID=:PRODUT' +
        'O_ID')
    Left = 208
    Top = 24
    ParamData = <
      item
        Name = 'PRODUTO_ID'
        DataType = ftInteger
        ParamType = ptInput
      end>
  end
end
